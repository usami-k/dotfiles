<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTDPLIST1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>io.github.usami-k.brewupdate</string>
        <key>ProgramArguments</key>
        <array>
            <string>/bin/sh</string>
            <string>-c</string>
            <string>echo ; date ; /usr/local/bin/brew update</string>
        </array>
        <dict>
            <key>LANG</key>
            <string>en_US.UTF-8</string>
            <key>HOME</key>
            <string>/Users/usami</string>
        </dict>
        <key>StandardOutPath</key>
        <string>$HOME/Library/Logs/io.github.usami-k.brewupdate.log</string>
        <key>StandardErrorPath</key>
        <string>$HOME/Library/Logs/io.github.usami-k.brewupdate.log</string>
        <key>RunAtLoad</key>
        <true/>
        <key>StartInterval</key>
        <integer>3600</integer>
    </dict>
</plist>
